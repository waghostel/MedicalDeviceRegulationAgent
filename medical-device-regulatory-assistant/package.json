{
  "name": "medical-device-regulatory-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:acceptance": "jest --testPathPatterns=acceptance",
    "test:accessibility": "jest --testPathPatterns=accessibility",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:visual": "playwright test e2e/visual",
    "test:e2e:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit --project=edge",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari --project=tablet-chrome --project=tablet-safari",
    "test:e2e:visual-update": "playwright test e2e/visual --update-snapshots",
    "test:performance": "jest --testPathPatterns=performance",
    "test:accessibility": "jest --testPathPatterns=accessibility",
    "test:all": "pnpm test:coverage && pnpm test:e2e && pnpm test:performance && pnpm test:accessibility",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "bundlesize": "bundlesize",
    "performance:monitor": "pnpm lighthouse && pnpm bundlesize",
    "visual:test": "pnpm test:e2e:visual",
    "visual:update": "pnpm test:e2e:visual-update",
    "visual:cross-browser": "pnpm test:e2e:cross-browser",
    "type-check": "tsc --noEmit",
    "maintenance:daily": "node scripts/testing-maintenance.js daily",
    "maintenance:weekly": "node scripts/testing-maintenance.js weekly",
    "maintenance:monthly": "node scripts/testing-maintenance.js monthly",
    "maintenance:validate-mock": "node scripts/testing-maintenance.js validate-mock-data",
    "maintenance:coverage": "node scripts/testing-maintenance.js analyze-coverage",
    "maintenance:performance": "node scripts/testing-maintenance.js check-performance",
    "maintenance:cleanup": "node scripts/testing-maintenance.js cleanup",
    "maintenance:health": "node scripts/testing-maintenance.js health-check"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
